{
  "name": "toolkitss",
  "version": "1.0.0",
  "description": "toolkitss",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npx lerna run build",
    "build:dev": "cross-env NODE_ENV=development npx lerna run build",
    "jest": "npx lerna run jest",
    "prepublish": "lerna run prepublish",
    "prettier": "prettier --write './packages/**/*.ts' './packages/**/*.tsx' --config ./.prettierrc",
    "publish-all": "yarn build && npx lerna publish patch -y --registry http://registry.npmjs.org",
    "lint-message": "echo 'Lint code before commit, refuse if error occurred'",
    "lint": "npx eslint ./**/*.ts",
    "fix": "npx eslint ./**/*.ts --fix",
    "start:app": "yarn --cwd ./app run update  && yarn --cwd ./app run start",
    "new": "npx ts-node --project scripts/tsconfig.json scripts/new/index.ts"
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "yarn prettier",
      "yarn fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunnyShining/toolkitss.git"
  },
  "author": "sunny<17765293970@163.com>",
  "bugs": {
    "url": "https://github.com/sunnyShining/toolkitss/issues"
  },
  "homepage": "https://github.com/sunnyShining/toolkitss#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "autoprefixer": "^9.7.3",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.1.0",
    "inquirer": "^7.0.0",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "lerna": "^3.19.0",
    "lerna-ci": "^0.0.4",
    "lint-staged": "^9.5.0",
    "postcss-clean": "^1.1.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">= 8.12.0",
    "npm": ">= 3.0.0",
    "yarn": ">=1.10.1"
  },
  "lerna-ci": {
    "syncremote": [
      "@toolkitss/helper",
      "@toolkitss/use-set-state"
    ]
  }
}